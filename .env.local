# Pandaura Frontend Environment Variables - Development
# Copy this file to .env.local for local development

# API Configuration (will auto-detect to localhost if not set)
VITE_API_BASE_URL=http://localhost:5000

# WebSocket Configuration
VITE_WS_BASE_URL=ws://localhost:5000

# Deployment Configuration
VITE_DEPLOYMENT_MODE=development
VITE_APP_TITLE=Pandaura AS (Local Development)

# Feature Flags
VITE_ANALYTICS_ENABLED=false
VITE_EXTERNAL_FONTS=true
VITE_CDN_ASSETS=false

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_UPLOAD_ENDPOINT=/api/upload
