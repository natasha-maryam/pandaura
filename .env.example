# Pandaura Frontend Environment Variables
# Copy this file to .env.local and configure for your environment

# API Configuration
VITE_API_BASE_URL=http://localhost:5000

# Deployment Configuration
VITE_DEPLOYMENT_MODE=production
VITE_APP_TITLE=Pandaura AS

# Feature Flags
VITE_ANALYTICS_ENABLED=false
VITE_EXTERNAL_FONTS=true
VITE_CDN_ASSETS=true

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_UPLOAD_ENDPOINT=/api/upload
