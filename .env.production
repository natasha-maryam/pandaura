# Pandaura Frontend Environment Variables - Production
# Copy this file to .env.local for production deployment

# API Configuration - Railway Backend
VITE_API_BASE_URL=https://pandaura-backend-production.up.railway.app

# WebSocket Configuration
VITE_WS_BASE_URL=wss://pandaura-backend-production.up.railway.app

# Deployment Configuration
VITE_DEPLOYMENT_MODE=production
VITE_APP_TITLE=Pandaura AS

# Feature Flags
VITE_ANALYTICS_ENABLED=true
VITE_EXTERNAL_FONTS=true
VITE_CDN_ASSETS=true

# File Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_UPLOAD_ENDPOINT=/api/v1/upload
